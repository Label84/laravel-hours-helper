{
    "name": "label84/laravel-hours-helper",
    "description": "Creates a Collection of times with a given interval.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Label84",
            "email": "info@label84.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Label84\\HoursHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Label84\\HoursHelper\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Label84\\HoursHelper\\HoursHelperServiceProvider"
            ],
            "aliases": {
                "HoursHelper": "Label84\\HoursHelper\\Facades\\HoursHelper"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "larastan/larastan": "^3.0",
        "phpunit/phpunit": "^11.5|^12",
        "laravel/pint": "^1.21"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
