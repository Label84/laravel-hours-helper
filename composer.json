{
    "name": "label84/laravel-hours-helper",
    "description": "Creates a Collection of times with a given interval.",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Label84",
        "email": "info@tjardo.com"
      }
    ],    
    "require": {
      "php": "^7.4 || ^8.0",
      "illuminate/support": "^8.0"
    },
    "autoload": {
      "psr-4": {
          "Label84\\HoursHelper\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
          "Label84\\HoursHelper\\Tests\\": "tests"
      }
    }, 
    "extra": {
      "laravel": {
          "providers": [
              "Label84\\HoursHelper\\HoursHelperServiceProvider"
          ]
      }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.5"
    }
}
